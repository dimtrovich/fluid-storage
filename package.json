{
  "name": "fluid-storage",
  "version": "1.0.0",
  "description": "A simple and fast client-side structured data storage interface",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build && npm test",
    "prepack": "npm run build",
	"check-publish": "npm pack --dry-run",
    "list-files": "npm pack --dry-run 2>/dev/null | head -n 10"
  },
  "keywords": [
    "localstorage",
    "storage",
    "sessionstorage",
    "cookie",
    "js",
    "typescript"
  ],
  "author": {
    "name": "Dimitri Sitchet Tomkeu",
    "email": "devcode.dst@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimtrovich/fluid-storage.git"
  },
  "bugs": {
    "url": "https://github.com/dimtrovich/fluid-storage/issues"
  },
  "homepage": "https://github.com/dimtrovich/fluid-storage#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^30.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "rollup": "^4.50.1",
    "rollup-plugin-dts": "^6.2.3",
    "ts-jest": "^29.4.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  }
}
